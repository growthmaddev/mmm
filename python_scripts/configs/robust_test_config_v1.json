{
    "data_filename": "attached_assets/dankztestdata_v2.csv",
    "dateColumn": "Date",
    "targetColumn": "Sales",
    "channelColumns": {
        "PPCBrand_Spend": "PPCBrand_Spend",
        "PPCNonBrand_Spend": "PPCNonBrand_Spend", 
        "PPCShopping_Spend": "PPCShopping_Spend",
        "PPCLocal_Spend": "PPCLocal_Spend",
        "PPCPMax_Spend": "PPCPMax_Spend",
        "FBReach_Spend": "FBReach_Spend",
        "FBDPA_Spend": "FBDPA_Spend",
        "OfflineMedia_Spend": "OfflineMedia_Spend"
    },
    "controlColumns": ["interestrate_control"],
    "eventColumns": [],
    "adstockSettings": {
        "default": {
            "adstock_alpha": 0.6,
            "adstock_l_max": 8
        },
        "channel_specific_params": {
            "PPCBrand_Spend": {
                "adstock_alpha": 0.8,
                "adstock_l_max": 4
            },
            "OfflineMedia_Spend": {
                "adstock_alpha": 0.3,
                "adstock_l_max": 12
            }
        }
    },
    "saturationSettings": {
        "default": {
            "saturation_L": 1.0,
            "saturation_x0": 50000.0,
            "saturation_k": 0.0001
        },
        "channel_specific_params": {
            "FBReach_Spend": {
                "saturation_L": 1.0,
                "saturation_x0": 10000.0,
                "saturation_k": 0.0002
            }
        }
    },
    "mcmc_settings": {
        "draws": 2000,
        "tune": 1000,
        "chains": 4,
        "cores": 2
    },
    "autoTransform": true,
    "transformTarget": "log",
    "scaleSpend": "standardize",
    "seasonality": {
        "extract_from_date": true,
        "features": ["month", "quarter", "is_holiday"]
    }
}